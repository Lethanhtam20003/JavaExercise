package Ex5_2;

public class Restaurant {
	private String name;
	private String food;
	private PriceRange price;
	private Intersection intersection;
	public Restaurant(String name, String food, PriceRange price, Intersection intersection) {
		super();
		this.name = name;
		this.food = food;
		this.price = price;
		this.intersection = intersection;
	}
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "name : "+ this.name + "food : " + this.food + " price : " + this.price + "intersection : "+this.intersection;
	}
}

package Ex5_2;

public class PriceRange {
	private int max;
	private int min;

	public PriceRange(int max, int min) {
		super();
		this.max = max;
		this.min = min;
	}

	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "max : " + this.max + "min :" + this.min;
	}

	@Override
	public boolean equals(Object obj) {
		// TODO Auto-generated method stub
		if (obj == null || !(obj instanceof PriceRange)) {
			return false;
		} else {

			PriceRange that = (PriceRange) obj;
			return this.max == that.max && this.min == that.min;
		}
	}
}
